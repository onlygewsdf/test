    if moveToBallIfTwoJump then
        MoveToBallTwoJumpButton.Text = "MOVE BALL 2 JUMP: ON"
        MoveToBallTwoJumpButton.BackgroundColor3 = Color3.fromRGB(40, 120, 40)
        showNotification("Move to Ball if 2 Jump ON", 2)
    else
