TiltMoveButton.MouseButton1Click:Connect(function()
    if not scriptEnabled then return end
    tiltMoveEnabled = not tiltMoveEnabled
    
    if tiltMoveEnabled then
        TiltMoveButton.Text = "MOVE TO TILT: ON"
        TiltMoveButton.BackgroundColor3 = Color3.fromRGB(40, 120, 40)
        tiltAdjustmentApplied = false
        showNotification("Move to Tilt ON", 2)
    else
        TiltMoveButton.Text = "MOVE TO TILT: OFF"
        TiltMoveButton.BackgroundColor3 = Color3.fromRGB(80, 120, 80)
        tiltAdjustmentApplied = false
        
        for playerName, marker in pairs(tiltMarkers) do
            if marker then
                marker:Destroy()
            end
        end
        tiltMarkers = {}
        lastTiltType = {}
        
        showNotification("Move to Tilt OFF", 2)
    end
end)

AutoPressVButton.MouseButton1Click:Connect(function()
    if not scriptEnabled then return end
    autoPressVEnabled = not autoPressVEnabled
    
    if autoPressVEnabled then
        AutoPressVButton.Text = "AUTO PRESS V: ON"
        AutoPressVButton.BackgroundColor3 = Color3.fromRGB(40, 120, 40)
        showNotification("Auto Press V ON", 2)
    else
        AutoPressVButton.Text = "AUTO PRESS V: OFF"
        AutoPressVButton.BackgroundColor3 = Color3.fromRGB(120, 80, 40)
        hasPressedV = {}
        vPressTimers = {}
        vKeyPressedTime = {}
        showNotification("Auto Press V OFF", 2)
    end
end)

