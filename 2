local currentTiltValue = 0
local maxTiltValue = 6000
local minTiltValue = 0
local sliderStep = 50
local isSliding = false

local function applyTiltToCharacter(value)
    local steppedValue = math.floor((math.clamp(value, minTiltValue, maxTiltValue) / sliderStep) + 0.5) * sliderStep
    currentTiltValue = steppedValue
    SliderTitle.Text = "Tilt Value: " .. tostring(currentTiltValue)
    TextBox.Text = tostring(currentTiltValue)
    SliderFill.Size = UDim2.new(currentTiltValue / maxTiltValue, 0, 1, 0)
    SliderButton.Position = UDim2.new(currentTiltValue / maxTiltValue, -10, 0.5, -10)

    if character and character:FindFirstChild("HumanoidRootPart") then
        local rootPart = character.HumanoidRootPart
        if rootPart:FindFirstChild("Tilt") then
            rootPart.Tilt.P = currentTiltValue
        end
    end
end

local function findPlayerByName(name)
    for _, player in pairs(Players:GetPlayers()) do
        if player.DisplayName:lower():find(name:lower()) then
            return player
        end
    end
    
    for _, player in pairs(Players:GetPlayers()) do
        if player.Name:lower():find(name:lower()) then
            return player
        end
    end
    return nil
end

local function findPlayersByTeam(teamName)
    local foundPlayers = {}
    for _, player in pairs(Players:GetPlayers()) do
        if player.Team and player.Team.Name:lower():find(teamName:lower()) then
            table.insert(foundPlayers, player.DisplayName)
        end
    end
    return foundPlayers
end

local function createPlayerButton(displayName, parentFrame)
    local player = nil
    for _, p in pairs(Players:GetPlayers()) do
        if p.DisplayName == displayName then
            player = p
            break
        end
    end
    
    if not player then return end
    
    local button = Instance.new("TextButton")
    button.Size = UDim2.new(1, 0, 1, 0)
    button.Text = displayName
    button.TextColor3 = Color3.new(1, 1, 1)
    button.TextSize = 10
    button.Font = Enum.Font.Gotham
    button.BackgroundColor3 = Color3.fromRGB(50, 50, 60)
    button.BorderSizePixel = 0
    button.TextTruncate = Enum.TextTruncate.AtEnd
    button.Parent = parentFrame
    
    button.MouseButton1Click:Connect(function()
        if enabledPlayers[displayName] then
            enabledPlayers[displayName] = false
            button.BackgroundColor3 = Color3.fromRGB(50, 50, 60)
            
            if playerMarkers[displayName] then
                playerMarkers[displayName]:Destroy()
                playerMarkers[displayName] = nil
            end
            if playerRays[displayName] then
                playerRays[displayName]:Destroy()
                playerRays[displayName] = nil
            end
            if tiltMarkers[displayName] then
                tiltMarkers[displayName]:Destroy()
                tiltMarkers[displayName] = nil
            end
            showNotification("Removed: " .. displayName, 2)
        else
            enabledPlayers[displayName] = true
            button.BackgroundColor3 = Color3.fromRGB(40, 120, 40)
            showNotification("Added: " .. displayName, 2)
        end
    end)
    
    button.MouseButton2Click:Connect(function()
        enabledPlayers[displayName] = false
        button.BackgroundColor3 = Color3.fromRGB(50, 50, 60)
        
        if playerMarkers[displayName] then
            playerMarkers[displayName]:Destroy()
            playerMarkers[displayName] = nil
        end
        if playerRays[displayName] then
            playerRays[displayName]:Destroy()
            playerRays[displayName] = nil
        end
        if tiltMarkers[displayName] then
            tiltMarkers[displayName]:Destroy()
            tiltMarkers[displayName] = nil
        end
        button:Destroy()
        showNotification("Removed: " .. displayName, 2)
    end)
    
    return button
end

local function clearPlayerGrid()
    for _, child in pairs(PlayerGrid:GetChildren()) do
        if child:IsA("TextButton") then
            child:Destroy()
        end
    end
end

local function updatePlayerGrid()
    clearPlayerGrid()
    
    local playerNames = {}
    for displayName, enabled in pairs(enabledPlayers) do
        table.insert(playerNames, displayName)
    end
    
    table.sort(playerNames)
    
    for i, displayName in ipairs(playerNames) do
        createPlayerButton(displayName, PlayerGrid)
    end
end

local function updateMoveDelaySlider(value)
    moveDelay = math.clamp(value, 0, 100)
    MoveDelayFill.Size = UDim2.new(moveDelay / 100, 0, 1, 0)
    MoveDelayLabel.Text = "Move Delay: " .. moveDelay .. " ms"
    MoveDelayValueBox.Text = tostring(moveDelay)
end

local function updateMaxDistanceSlider(value)
    maxDistance = math.clamp(value, 1, 100)
    MaxDistanceFill.Size = UDim2.new(maxDistance / 100, 0, 1, 0)
    MaxDistanceLabel.Text = "Max Distance: " .. maxDistance .. " studs"
    MaxDistanceValueBox.Text = tostring(maxDistance)
end

local function updateMinMoveDistanceSlider(value)
    minMoveDistance = math.clamp(value, 0.1, 20)
    MinMoveDistanceFill.Size = UDim2.new(minMoveDistance / 20, 0, 1, 0)
    MinMoveDistanceLabel.Text = "Min Move Distance: " .. minMoveDistance .. " studs"
    MinMoveDistanceValueBox.Text = tostring(minMoveDistance)
end

local function updateApproachDurationSlider(value)
    approachDurationSetting = math.clamp(value, 0.1, 5)
    approachDuration = approachDurationSetting
    ApproachDurationFill.Size = UDim2.new(approachDurationSetting / 5, 0, 1, 0)
    ApproachDurationLabel.Text = "Approach Duration: " .. approachDurationSetting .. " sec"
    ApproachDurationValueBox.Text = tostring(approachDurationSetting)
end

local function updateClickRadiusSlider(value)
    clickRadius = math.clamp(value, 1, 20)
    ClickRadiusFill.Size = UDim2.new(clickRadius / 20, 0, 1, 0)
    ClickRadiusLabel.Text = "Click Radius: " .. clickRadius .. " studs"
    ClickRadiusValueBox.Text = tostring(clickRadius)
end

local function updateDistanceToPressVSlider(value)
    distanceToPressV = math.clamp(value, 1, 100)
    DistanceToPressVFill.Size = UDim2.new(distanceToPressV / 100, 0, 1, 0)
    DistanceToPressVLabel.Text = "Distance to Press V: " .. distanceToPressV .. " studs"
    DistanceToPressVValueBox.Text = tostring(distanceToPressV)
end

local function updateVPressDelaySlider(value)
    vPressDelay = math.clamp(value, 0.1, 2)
    VPressDelayFill.Size = UDim2.new(vPressDelay / 2, 0, 1, 0)
    VPressDelayLabel.Text = "V Press Delay: " .. vPressDelay .. " sec"
    VPressDelayValueBox.Text = tostring(vPressDelay)
end

local configuringHotkey = nil

local function finishHotkeyConfiguration(keyCode)
    if configuringHotkey and keyCode then
        Hotkeys[configuringHotkey] = keyCode
        if hotkeyButtons[configuringHotkey] then
            hotkeyButtons[configuringHotkey].Text = tostring(keyCode):gsub("Enum.KeyCode.", "")
            hotkeyButtons[configuringHotkey].BackgroundColor3 = Color3.fromRGB(30, 30, 40)
        end
        configuringHotkey = nil
        showNotification("Hotkey set: " .. tostring(keyCode):gsub("Enum.KeyCode.", ""), 2)
    end
end

local function setupSlider(sliderFrame, fillFrame, updateFunction, maxValue)
    local dragging = false
    
    sliderFrame.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true
        end
    end)
    
    sliderFrame.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = false
        end
    end)
    
    sliderFrame.MouseMoved:Connect(function()
        if dragging then
            local mousePos = UserInputService:GetMouseLocation()
            local sliderAbsPos = sliderFrame.AbsolutePosition
            local sliderAbsSize = sliderFrame.AbsoluteSize
            
            local relativeX = (mousePos.X - sliderAbsPos.X) / sliderAbsSize.X
            relativeX = math.clamp(relativeX, 0, 1)
            
            updateFunction(math.floor(relativeX * maxValue))
        end
    end)
end

