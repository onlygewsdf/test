UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    
    if configuringHotkey then
        finishHotkeyConfiguration(input.KeyCode)
        return
    end
    
    if input.KeyCode == Hotkeys.ToggleMenu then
        MainFrame.Visible = not MainFrame.Visible
        showNotification(MainFrame.Visible and "Menu shown" or "Menu hidden", 1.5)
    end
end)

setupSlider(MoveDelaySlider, MoveDelayFill, updateMoveDelaySlider, 100)
setupSlider(MaxDistanceSlider, MaxDistanceFill, updateMaxDistanceSlider, 100)
setupSlider(MinMoveDistanceSlider, MinMoveDistanceFill, updateMinMoveDistanceSlider, 20)
setupSlider(ApproachDurationSlider, ApproachDurationFill, updateApproachDurationSlider, 5)
setupSlider(ClickRadiusSlider, ClickRadiusFill, updateClickRadiusSlider, 20)
setupSlider(DistanceToPressVSlider, DistanceToPressVFill, updateDistanceToPressVSlider, 100)
setupSlider(VPressDelaySlider, VPressDelayFill, updateVPressDelaySlider, 2)

