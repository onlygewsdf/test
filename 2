PanicButton.MouseButton1Click:Connect(function()
    scriptEnabled = false
    autoMoveEnabled = false
    moveToBallEnabled = false
    moveToBallIfTwoJump = false
    tiltMoveEnabled = false
    autoPressVEnabled = false
    autoRecEnabled = false
    autoDiveEnabled = false
    
    stopMovement()
    cleanupBallMarkers()
    
    for playerName, marker in pairs(playerMarkers) do
        if marker then
            marker:Destroy()
        end
    end
    for playerName, ray in pairs(playerRays) do
        if ray then
            ray:Destroy()
        end
    end
    for playerName, marker in pairs(tiltMarkers) do
        if marker then
            marker:Destroy()
        end
    end
    
    playerMarkers = {}
    playerRays = {}
    tiltMarkers = {}
    enabledPlayers = {}
    lastJumpTime = {}
    approachStartTimes = {}
    hasPressedV = {}
    vPressTimers = {}
    vKeyPressedTime = {}
    lastTiltType = {}
    
    local safeMarker = workspace:FindFirstChild("SafePositionMarker")
    if safeMarker then
        safeMarker:Destroy()
    end
    
    ScreenGui:Destroy()
    
    showNotification("Script disabled - Panic Mode", 3)
end)

AutoTeamButton.MouseButton1Click:Connect(function()
    if not scriptEnabled then return end
    local detectedTeam = detectCurrentTeam()
    setTeam(detectedTeam)
    TeamStatus.Text = "Current Team: " .. detectedTeam
    if detectedTeam == "Blue" then
        TeamStatus.TextColor3 = Color3.fromRGB(100, 150, 255)
    elseif detectedTeam == "Red" then
        TeamStatus.TextColor3 = Color3.fromRGB(255, 100, 100)
    else
        TeamStatus.TextColor3 = Color3.new(1, 1, 1)
    end
    showNotification("Auto-detected: " .. detectedTeam .. " team", 2)
end)

AntiBackButton.MouseButton1Click:Connect(function()
    if not scriptEnabled then return end
    autoAntiBackEnabled = not autoAntiBackEnabled
    if autoAntiBackEnabled then
        AntiBackButton.Text = "ANTI BACK: ON"
        AntiBackButton.BackgroundColor3 = Color3.fromRGB(40, 120, 40)
        showNotification("Anti-Back ON", 2)
    else
        AntiBackButton.Text = "ANTI BACK: OFF"
        AntiBackButton.BackgroundColor3 = Color3.fromRGB(120, 60, 40)
        showNotification("Anti-Back OFF", 2)
    end
end)

