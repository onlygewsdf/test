LoadTeamButton.MouseButton1Click:Connect(function()
    if not scriptEnabled then return end
    local teamName = TeamTextBox.Text
    if teamName == "" then 
        showNotification("Enter a team name", 2)
        return 
    end
    
    local teamPlayers = findPlayersByTeam(teamName)
    
    if #teamPlayers == 0 then
        showNotification("No players found in team: " .. teamName, 2)
        return
    end
    
    for i, displayName in ipairs(teamPlayers) do
        enabledPlayers[displayName] = true
    end
    
    updatePlayerGrid()
    showNotification("Loaded " .. #teamPlayers .. " players from " .. teamName, 2)
end)

LoadPlayerButton.MouseButton1Click:Connect(function()
    if not scriptEnabled then return end
    local playerName = PlayerTextBox.Text
    if playerName == "" then 
        showNotification("Enter a player name", 2)
        return 
    end
    
    local targetPlayer = findPlayerByName(playerName)
    if targetPlayer then
        enabledPlayers[targetPlayer.DisplayName] = true
        updatePlayerGrid()
        showNotification("Added player: " .. targetPlayer.DisplayName, 2)
    else
        showNotification("Player not found: " .. playerName, 2)
    end
end)

ClearAllButton.MouseButton1Click:Connect(function()
    if not scriptEnabled then return end
    
    enabledPlayers = {}
    lastJumpTime = {}
    approachStartTimes = {}
    
    for displayName, marker in pairs(playerMarkers) do
        if marker then
            marker:Destroy()
        end
    end
    for displayName, ray in pairs(playerRays) do
        if ray then
            ray:Destroy()
        end
    end
    for displayName, marker in pairs(tiltMarkers) do
        if marker then
            marker:Destroy()
        end
    end
    
    playerMarkers = {}
    playerRays = {}
    tiltMarkers = {}
    lastTiltType = {}
    
    clearPlayerGrid()
    showNotification("Cleared all players", 2)
end)

LoadAllButton.MouseButton1Click:Connect(function()
    if not scriptEnabled then return end
    
    enabledPlayers = {}
    lastJumpTime = {}
    approachStartTimes = {}
    
    for _, player in pairs(Players:GetPlayers()) do
        if player ~= PLAYER then
            enabledPlayers[player.DisplayName] = true
        end
    end
    
    updatePlayerGrid()
    showNotification("Loaded all players: " .. #Players:GetPlayers() - 1, 2)
end)

SetRaycastButton.MouseButton1Click:Connect(function()
    if not scriptEnabled then return end
    local newLength = tonumber(RaycastTextBox.Text)
    if newLength and newLength >= 3 and newLength <= 100 then
        raycastLength = newLength
        RaycastLabel.Text = "Raycast Length: " .. raycastLength
        showNotification("Raycast set to " .. raycastLength, 2)
    end
end)

